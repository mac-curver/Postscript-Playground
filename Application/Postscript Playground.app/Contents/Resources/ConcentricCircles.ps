/pagewidth  currentpagedevice /PageSize get 0 get def
/pageheight currentpagedevice /PageSize get 1 get def


0.0 setgray

/margin pagewidth 0.170 mul def
/inner 5 def
/factor 0.952 def
/number 25 def



/xarray [margin pagewidth  margin sub pagewidth  0.5 mul] def
/yarray [margin pageheight margin sub pageheight 0.5 mul] def


% X-center Y-center inner-Diameter number-of-circles concentric
/concentric {
    /number exch def
    /diameter exch def
    /yc exch def
    /xc exch def
    xc yc diameter 0 360 arc closepath
    fill

    number {
        /diameter diameter delta add def
        /delta delta factor mul def
        xc yc diameter 0 360 arc closepath
        /diameter diameter delta add def
        /delta delta factor mul def
        xc yc diameter 360 0 arcn closepath
        fill
    }  repeat
} def

yarray {
    /ycenter exch def
    xarray {
        /xcenter exch def
        /center {xcenter ycenter} def
	    /diameter inner def
        /delta diameter def

        center diameter number concentric
    } forall
} forall


showpage 

